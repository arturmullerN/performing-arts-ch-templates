		<http://www.metaphacts.com/ontology/fields#insertPattern> _:genid-7664e0cf5ebd444d91fb2b0aeaa92280-045BB21BF859979818FD984DAD7B52D0 ;
		<http://www.metaphacts.com/ontology/fields#selectPattern> _:genid-7664e0cf5ebd444d91fb2b0aeaa92280-31B1F1B42A4BB5E80AF5495C262FFE8A ;
		<http://www.metaphacts.com/ontology/fields#xsdDatatype> xsd:anyURI .
	
	_:genid-7664e0cf5ebd444d91fb2b0aeaa92280-045BB21BF859979818FD984DAD7B52D0 a sp:Query ;
		sp:text """INSERT {
  $subject owl:sameAs $value .
} WHERE {}""" .
	
	_:genid-7664e0cf5ebd444d91fb2b0aeaa92280-31B1F1B42A4BB5E80AF5495C262FFE8A a sp:Query ;
		sp:text """SELECT ?value ?label
WHERE {
  $subject owl:sameAs ?value .

  BIND (REPLACE(STR(?value), \".*\\\\/c*b*\", \"\") as ?localname)
  BIND (STRBEFORE(STR(?value), ?localname) as ?prefix)

  VALUES (?prefix ?source ?order)
  {
    (\"http://www.wikidata.org/entity/\" \"Wikidata\" 0)
    (\"http://www.isni.org/\" \"ISNI\" 1)
    (\"http://viaf.org/viaf/\" \"VIAF\" 2)
    (\"http://d-nb.info/gnd/\" \"GND\" 3)
    (\"https://catalogue.bnf.fr/ark:/12148/cb\" \"BnF\" 4)
    (\"http://geonames.org/\" \"Geonames\" 5)
    (\"http://classifications.data.admin.ch/municipality/\" \"Swiss Municipality\" 6)
    (\"http://classifications.data.admin.ch/canton/\" \"Swiss Municipality\" 7)
    (\"http://tls.theaterwissenschaft.ch/wiki/\" \"Theaterlexikon der Schweiz\" 8)
  }
  BIND(IF(?order<8, CONCAT(?source,\": \",?localname), ?source) as ?label)
} ORDER BY ASC(?order)""" .
	
	_:genid-7664e0cf5ebd444d91fb2b0aeaa92280-DE94D7D8AABA032BB423567BF5FB4180 a sp:Query ;
		sp:text """DELETE {
  $subject owl:sameAs $value .
} WHERE { }""" .
}

